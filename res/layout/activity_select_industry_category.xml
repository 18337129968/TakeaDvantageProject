<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/apply_for_merchant_state_fail"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/im_examine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="35dp"
            android:background="@drawable/btn_reviewf" />

        <TextView
            android:id="@+id/tv_examine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="30dp"
            android:text="未通过审核！"
            android:textColor="@color/common"
            android:textSize="35dp" />

        <TextView
            android:id="@+id/tv_examine2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="15dp"
            android:text="您所输入的名字和身份证名称不符"
            android:textColor="@color/common"
            android:textSize="18dp" />

        <Button
            android:id="@+id/btn_examine"
            android:layout_width="240dp"
            android:layout_height="40dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="100dp"
            android:background="@drawable/merchant_account_btn_bottom"
            android:text="重新申请"
            android:textColor="@color/edittext_color" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="right|bottom"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="10dp"
            android:gravity="bottom"
            android:text="客服电话:010-8264886"
            android:textColor="#85FFFFFF"
            android:textSize="15dp" />
    </LinearLayout>

</LinearLayout>